{
    "name": "benjaminlazarecki/scarepiceditor-bundle",
    "type": "symfony-bundle",
    "description": "Provides an EpicEditor integration for your Symfony2 Project.",
    "keywords": ["epiceditorbundle"],
    "license": "MIT",
    "authors": [
        {
            "name": "Benjamin Lazarecki",
            "email": "benjamin.lazarecki@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/framework-bundle": ">=2.1",
        "symfony/form": ">=2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "twig/twig": "1.*"
    },
    "suggest": {
        "twig/twig": "Allows to use ScarEpicEditor twig templates"
    },
    "autoload": {
        "psr-0": { "Scar\\EpicEditorBundle": "" }
    },
    "config": {
        "bin-dir": "bin/"
    },
    "target-dir": "Scar/EpicEditorBundle"
}
